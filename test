import "ifj25" for Ifj
class Program {
    static main() {
        var a
        var b
        var c
        var r

        a = "abcd"
        b = 0
        c = 2

        a = 123           // prvý param int
        b = "0"           // druhý param string
        c = 1 < 2         // tretí param bool

        r = Ifj.substring(a, b, c)   // vetko zlé 25

        __d = Ifj.write(r)
    }
}
